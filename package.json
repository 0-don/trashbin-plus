{
  "name": "trashbin-plus",
  "version": "1.0.9",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc --noEmit && bun build.ts",
    "build:local": "tsc --noEmit && bun build.ts --out=dist --minify",
    "dev": "concurrently \"bun build.ts --watch\" \"spicetify watch -e\"",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "i18next": "25.8.4",
    "react-i18next": "16.5.4",
    "onnxruntime-web": "1.24.1",
    "react-icons": "5.5.0",
    "zustand": "5.0.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@types/react": "18.3.26",
    "@types/react-dom": "18.3.7",
    "@types/bun": "1.3.9",
    "concurrently": "9.2.1",
    "postcss": "8.5.6",
    "postcss-load-config": "6.0.1",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3"
  }
}
