{
  "name": "trashbin-plus",
  "version": "1.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "spicetify-creator",
    "build:local": "spicetify-creator --out=dist --minify",
    "dev:fix": "rm -rf yarn.lock && rm -rf node_modules && yarn && yarn cache clean && yarn --force && concurrently \"spicetify-creator --watch\" \"spicetify watch -e\"",
    "dev": "concurrently \"spicetify-creator --watch\" \"spicetify watch -e\"",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@tanstack/react-virtual": "3.13.12",
    "i18next": "25.6.3",
    "react-i18next": "16.3.5",
    "react-icons": "5.5.0",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.17",
    "@types/react": "18.3.26",
    "@types/react-dom": "18.3.7",
    "concurrently": "9.2.1",
    "prettier": "3.7.1",
    "prettier-plugin-tailwindcss": "0.7.1",
    "spicetify-creator": "1.0.17",
    "tailwindcss": "4.1.17"
  }
}
