  "scripts": {
    "build": "spicetify-creator",
    "build-local": "spicetify-creator --out=dist --minify",
    "dev:fix": "rm -rf yarn.lock && rm -rf node_modules && yarn && yarn cache clean && yarn --force && concurrently \"spicetify-creator --watch\" \"spicetify watch -e\"",
    "dev": "concurrently \"spicetify-creator --watch\" \"spicetify watch -e\""
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.16",
    "@types/minimatch": "^6.0.0",
    "@types/react": "18.3.26",
    "@types/react-dom": "18.3.7",
    "concurrently": "9.2.1",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.7.1",
    "spicetify-creator": "github:0-don/spicetify-creator",
    "tailwindcss": "4.1.8",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@tanstack/react-virtual": "3.13.10",
    "i18next": "25.6.0",
    "react-i18next": "16.2.4",
    "react-icons": "5.5.0",
    "zustand": "5.0.8"
  }
}
