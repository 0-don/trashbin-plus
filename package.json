{
  "name": "trashbin-plus",
  "version": "1.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc --noEmit && spicetify-creator",
    "build:local": "tsc --noEmit && spicetify-creator --out=dist --minify",
    "dev:fix": "rm -rf yarn.lock && rm -rf node_modules && yarn && yarn cache clean && yarn --force && concurrently \"spicetify-creator --watch\" \"spicetify watch -e\"",
    "dev": "concurrently \"npx spicetify-creator --watch\" \"spicetify watch -e\"",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "i18next": "25.7.3",
    "react-i18next": "16.5.0",
    "react-icons": "5.5.0",
    "zustand": "5.0.9"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@types/react": "18.3.26",
    "@types/react-dom": "18.3.7",
    "concurrently": "9.2.1",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "0.7.2",
    "spicetify-creator": "github:0-don/spicetify-creator",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3"
  }
}
